# Database - Replace with your actual PostgreSQL connection string
# Local example: postgresql://postgres:password@localhost:5432/manakhaah
# Supabase example: postgresql://postgres:[YOUR-PASSWORD]@db.[PROJECT-REF].supabase.co:5432/postgres
DATABASE_URL="postgresql://postgres:password@localhost:5432/manakhaah"

# NextAuth - Generate secret with: openssl rand -base64 32
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-secret-key-here-generate-with-openssl-rand-base64-32"

# Mapbox (Optional - get free token from https://mapbox.com)
NEXT_PUBLIC_MAPBOX_TOKEN=""

# Cloudinary (Optional - get free account from https://cloudinary.com)
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=""
CLOUDINARY_API_KEY=""
CLOUDINARY_API_SECRET=""

# Resend (Optional - get free account from https://resend.com)
RESEND_API_KEY=""

# Bay Area Default Location (Fremont, CA)
NEXT_PUBLIC_DEFAULT_LAT="37.5485"
NEXT_PUBLIC_DEFAULT_LNG="-121.9886"
NEXT_PUBLIC_DEFAULT_CITY="Fremont, CA"
